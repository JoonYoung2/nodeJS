<table border="1">
    <tr>
        <th>번호</th>
        <th>이름</th>
        <th>가격</th>
        <th>number</th>
        <th>total</th>
    </tr>
<%
    list.forEach(item => {
%>
    <tr>
        <td><%=item.goods_id%></td>
        <td><%=item.title%></a></td>
        <td><%=item.price%>원</td>
        <td><%=item.number%>개</td>
        <td><%=item.total%>원</td>
    </tr>
<%
    })
%>
</table>

